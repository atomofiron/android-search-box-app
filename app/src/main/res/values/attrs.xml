<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="isDarkTheme" format="boolean" />
    <attr name="isBlackDeep" format="boolean" />
    <attr name="colorBackground" format="color" />
    <attr name="colorContent" format="color" />
    <attr name="colorFieldBackground" format="color" />
    <attr name="colorFieldDivider" format="color" />
    <attr name="colorBone" format="color" />
    <attr name="colorOverlay" format="color" />
    <attr name="topRadioGroupBackground" format="color" />

    <declare-styleable name="VerticalDockView">
        <attr name="title" format="string" />
    </declare-styleable>

    <declare-styleable name="AutoHideKeyboardField">
        <attr name="hideKeyboardOnFocusLost" format="boolean" />
    </declare-styleable>

    <declare-styleable name="BallsView">
        <attr name="oneBall" format="boolean" />
    </declare-styleable>

    <declare-styleable name="SystemUiBackgroundView">
        <attr name="statusBar" format="boolean" />
        <attr name="navigationBar">
            <flag name="none" value="0x0" />
            <flag name="all" value="0x13" />
            <flag name="start" value="0x1" />
            <flag name="end" value="0x4" />
            <flag name="horizontal" value="0x5" />
            <flag name="bottom" value="0x8" />
        </attr>
    </declare-styleable>
</resources>